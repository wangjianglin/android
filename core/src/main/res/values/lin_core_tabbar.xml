    <!-- TabBar 相关id 开始 -->
    <item type="id" name="lin_core_tabbar_bar_layout" />
    <item type="id" name="lin_core_tabbar_content" />
    <item type="id" name="lin_core_tabbar_main_bottom" />
    <item type="id" name="lin_core_tabbar_tiem_bar_img_id" />
    <item type="id" name="lin_core_tabbar_tiem_bar_text_id" />
    <item type="id" name="lin_core_tabbar_tiem_bar_overlay_view_id" />

    <!-- TabBar 相关id 结束 -->

